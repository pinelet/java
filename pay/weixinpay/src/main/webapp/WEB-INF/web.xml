<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>MainService</servlet-name>
  	<display-name>MainService</display-name>
  	<description></description>
  	<servlet-class>com.pinelet.weixinpay.servlet.MainService</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>VaildService</servlet-name>
  	<display-name>VaildService</display-name>
  	<description></description>
  	<servlet-class>com.pinelet.weixinpay.servlet.VaildService</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MainService</servlet-name>
  	<url-pattern>/wx/main</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>VaildService</servlet-name>
  	<url-pattern>/wx/vaild</url-pattern>
  </servlet-mapping>
</web-app>
